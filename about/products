      data-g-label="share:{[::feed.url]}"
        data-g-href="{[::feed.url]}"
>
                <div class="carousel-slide-media carousel-slide-bg image-placeholder" data-ng-if="!feed.image && !feed.video">
                  <img class="{[::feed.type]}" data-ng-src="/assets/img/{[feed.image_generic]}">
                  <div data-ng-if="feed.type == 'social'" class="account-name">{[feed.network === newsFeedCtrl.network.GOOGLE_PLUS ? '+' : '@']}{[::feed.account | stripSpaces]}</div>
                </div>
                <div class="carousel-slide-media carousel-slide-bg" data-ng-style="{'background-image': 'url({[::feed.image]})'}" data-ng-if="feed.image && (!feed.video || !mainCtrl.detect.INLINE_VIDEO)">
                  <img class="{[::feed.type]} backup-image" data-ng-src="/assets/img/blog-fallback-3.svg">
                </div>
                <div class="carousel-slide-media video-container" data-ng-if="feed.video && mainCtrl.detect.INLINE_VIDEO">
                  <video data-ng-if="feed.video" data-ng-src="{[::feed.video]}" height="100%" width="100%" loop autoplay data-mute-video><!----></video>
                </div>
                <div class="carousel-slide-content">
                  <div data-ng-if="feed.type == 'social'" class="icon-outer">
