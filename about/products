        <ul class="glue-carousel" data-glue-carousel-options="mainCtrl.carouselOptions" data-carousel-tracking="Product News">
          <li class="carousel-slide" data-glue-carousel-repeat="feed in ::newsFeedCtrl.feed" data-ng-class="[feed.type, feed.network_class, feed.account_slug, {'no-post-image': !feed.image}]">
